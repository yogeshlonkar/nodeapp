{
  "name": "nodeapp",
  "version": "1.0.0",
  "description": "Nodejs application",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "link-module-alias",
    "start": "pm2 start src/index.js",
    "dev": "source .env && nodemon --inspect src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yogeshlonkar/nodeapp.git"
  },
  "keywords": [
    "react",
    "app"
  ],
  "author": "Yogesh Lonkar <lonkar.yogeshr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yogeshlonkar/nodeapp/issues"
  },
  "homepage": "https://github.com/yogeshlonkar/nodeapp#readme",
  "config": {
    "unsafe-perm": true
  },
  "_moduleAliases": {
    "root": ".",
    "src": "src",
    "dao": "src/dao"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "jsonschema": "^1.2.4",
    "link-module-alias": "^1.1.5",
    "mongodb": "^3.1.4",
    "pm2": "^3.0.4"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.3"
  }
}
